{
  "name": "twikoo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "postinstall": "patch-package",
    "diff":"patch-package tkserver",
    "build": "rimraf ./dist && pkg . -C GZip --no-bytecode --public-packages \"*\" --public"
  },
  "pkg": {
    "scripts": [],
    "assets": [
      "./node_modules/axios/dist/node/axios.cjs",
      "./node_modules/@imaegoo/node-ip2region/data/ip2region.db",
      "./web.config",
      "./.env"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.4.1",
    "tkserver": "1.6.41"
  },
  "devDependencies": {
    "patch-package": "^8.0.0",
    "pkg": "5.8.1"
  }
}
